#include <iostream>

using namespace std;

int main()
{
	double a = 0;
	double b = 0;

	cout << "Veddite a: ";
	cin >> a;
	cout << "Veddite b: ";
	cin >> b;


	cout << "Summa = " << (a * a) + (b * b) << "\n";
	cout << "Rashnost = " << (a * a) - (b * b) << "\n";
	cout << "Proisvedenie = " << (a * a) * (b * b) << "\n";
	cout << "Chastnoe = " << (a * a) / (b * b) << "\n";
}
