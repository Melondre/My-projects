#include <iostream>

using namespace std;

int main()
{
	double a, b;

	cout << "Veddite a: ";
	cin >> a;
	cout << "Veddite b: ";
	cin >> b;

	if (!a || !b) exit(1);
	a = abs(a);
	b = abs(b);

	cout << "Summa = " << a + b << "\n";
	cout << "Rashnost = " << a - b << "\n";
	cout << "Proisvedenie = " << a * b << "\n";
	cout << "Chastnoe = " << a / b << "\n";
}
